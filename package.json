{
  "name": "@fqqb/timeline",
  "version": "1.0.1",
  "description": "Render events on an interactive timeline chart",
  "author": "fqqb",
  "license": "MIT",
  "repository": "fqqb/timeline",
  "homepage": "https://fqqb.github.io/timeline",
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "watch": "rollup -cw",
    "tsdoc": "typedoc --entryPoints ./src --excludePrivate --json docs/_data/tsdoc.json",
    "serve": "cd docs; bundle exec jekyll serve --config _config.dev.yml",
    "jekyll": "npm run tsdoc && npm run serve",
    "dev": "npx concurrently -n rollup,jekyll -c yellow,magenta npm:watch npm:jekyll"
  },
  "main": "dist/timeline.umd.js",
  "module": "dist/timeline.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/luxon": "^1.27.1",
    "@types/node": "^14.17.4",
    "luxon": "^2.0.1",
    "rollup": "^2.52.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "typedoc": "^0.21.4",
    "typescript": "^4.3.4"
  }
}
