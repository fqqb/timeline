{
  "name": "@fqqb/timeline",
  "version": "1.2.0",
  "description": "Render events on an interactive timeline chart",
  "author": "fqqb",
  "license": "MIT",
  "repository": "fqqb/timeline",
  "homepage": "https://fqqb.github.io/timeline",
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "watch": "rollup -cw",
    "tsdoc": "typedoc --entryPoints ./src --json docs/_data/tsdoc.json",
    "serve": "cd docs; bundle exec jekyll serve",
    "jekyll": "npm run tsdoc && npm run serve",
    "dev": "npx concurrently -n rollup,jekyll -c yellow,magenta npm:watch npm:jekyll"
  },
  "main": "dist/timeline.umd.js",
  "module": "dist/timeline.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/luxon": "^2.3.2",
    "@types/node": "^14.18.20",
    "luxon": "^2.4.0",
    "rollup": "^2.75.5",
    "rollup-plugin-terser": "^7.0.2",
    "terser": "^5.14.0",
    "tslib": "^2.4.0",
    "typedoc": "^0.21.04",
    "typescript": "^4.7.3"
  }
}
