{
  "name": "@fqqb/timeline",
  "version": "1.0.0",
  "description": "Render events on an interactive timeline chart",
  "author": "fqqb",
  "license": "MIT",
  "repository": "fqqb/timeline",
  "homepage": "https://fqqb.github.io/timeline",
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "watch": "rollup -cw",
    "tsdoc": "npx typedoc --excludePrivate --json docs/_data/tsdoc.json",
    "serve": "cd docs; bundle exec jekyll serve",
    "jekyll": "npm run tsdoc && npm run serve",
    "dev": "npx concurrently -n rollup,jekyll -c yellow,magenta npm:watch npm:jekyll"
  },
  "main": "dist/timeline.umd.js",
  "module": "dist/timeline.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@types/node": "^14.14.41",
    "rollup": "^2.45.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.2.4"
  }
}
