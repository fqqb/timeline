{
  "name": "timenav",
  "version": "1.0.0",
  "description": "Render events on an interactive timeline chart",
  "author": "fqqb",
  "license": "MIT",
  "repository": "fqqb/timenav.js",
  "homepage": "https://fqqb.github.io/timenav.js",
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "watch": "rollup -cw",
    "tsdoc": "npx typedoc --excludePrivate --json docs/_data/tsdoc.json",
    "serve": "cd docs; bundle exec jekyll serve",
    "jekyll": "npm run tsdoc && npm run serve",
    "dev": "npx concurrently -n rollup,jekyll -c yellow,magenta npm:watch npm:jekyll"
  },
  "main": "dist/timenav.umd.js",
  "module": "dist/timenav.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@types/node": "^12.12.21",
    "rollup": "^1.27.13",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-typescript2": "^0.25.3",
    "typescript": "^3.7.3"
  }
}
